{
    "name": "bedita/tus",
    "description": "BEdita/Tus plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "LGPL-3.0-or-later",
    "require": {
        "bedita/core": "*",
        "bedita/api": "*",
        "ankitpokhrel/tus-php": "^2.2",
        "bedita/aws": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0",
        "cakephp/cakephp-codesniffer": "~3.2.1"
    },
    "autoload": {
        "psr-4": {
            "BEdita\\Tus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BEdita\\Tus\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    },
    "scripts": {
        "cs-check": "vendor/bin/phpcs -p src/ tests/",
        "cs-fix": "vendor/bin/phpcbf -p src/ tests/"
    }
}
